All: main modulo encryptArray arrayIntoFile moduloPower encrypt

LIB=libConversion.o
CC=gcc

arrayIntoFile: arrayIntoFile.o $(LIB)
	$(CC) $@.o $(LIB) -g -o $@

encryptArray: encryptArray.o $(LIB)
	$(CC) $@.o $(LIB) -g -o $@

modulo: modulo.o $(LIB)
	$(CC) $@.o $(LIB) -g -o $@

moduloPower: moduloPower.o $(LIB)
	$(CC) $@.o $(LIB) -g -o $@

encrypt: encrypt.o $(LIB)
	$(CC) $@.o $(LIB) -g -o $@

main: main.o $(LIB)   # Add $(LIB) as a dependency
	$(CC) $@.o $(LIB) -g -o $@

.s.o:
	$(CC) $(@:.o=.s) -g -c -o $@
